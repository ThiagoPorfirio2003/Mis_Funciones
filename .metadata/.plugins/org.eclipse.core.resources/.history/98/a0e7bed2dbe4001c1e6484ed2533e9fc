#include <stdlib.h>

int static swapReversaInt(int* punteroInt)
{
	int retorno;
	int auxiliar;

	retorno=1;

	if(punteroInt != NULL)
	{
		auxiliar = *(punteroInt-1);
		*(punteroInt-1)= *punteroInt;
		*punteroInt=auxiliar;
		retorno=0;
	}

	return retorno;
}

int static swapDelantero()
{

}
